<div class="product-list-wrapper">
	<div class="list-title-wrapper">
		<h3 class="list-title">CCTV Cameras</h3>
	</div>
	<div class="list-container">
		<ul style="list-style: None" class="list">
			<% let prodName; %>
				<% let prodCode; %>
					<% data.forEach(function(el){ %>
						<% data[2].forEach(function(element){ %>
							<% prodName=element; %>
								<% }) %>
									<% data[3].forEach(function (element){ %>
										<% prodCode=element; %>
											<% }) %>
												<li>
													<a href="<%= `/product-page/${prodCode}` %>" </a>
														<%= prodName %>
													</a>
												</li>
												<% }); %>
		</ul>
	</div>
</div>